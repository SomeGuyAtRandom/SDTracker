<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="Signal Design Tracking System">
    <meta name="author" content="Ray Andrade">
    <link rel="icon" href="../../favicon.ico">
    <title>@ViewBag.Title</title>
    
    @Styles.Render("~/styles/bootstrap")
    @Scripts.Render("~/bundles/jquery")
    
    <link href="~/Content/themes/base/autocomplete.css" rel="stylesheet" />
    <script src="~/Scripts/bootstrap.min.js"></script>
    @Scripts.Render("~/bundles/jqueryvalidate")
   
    
    
</head>

<body>

    <!-- Fixed navbar -->
    <nav class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Signal Design Tracking Application", "TestPage", "Home", null, new { @class = "navbar-brand" })
            </div>
            <div id="navbar" class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    
                    
                    <li class="active">
                        @Html.ActionLink("Home", "Index", "Home")
                    </li>
                    
                    <li></li> 
                    @if (Request.IsAuthenticated)
                    {

    <li>
        @Html.Partial(@"MenuBarItems/_AdminUser")
    </li>

    <li>
        @Html.Partial(@"MenuBarItems/_Projects")
    </li>

    <li>
        @Html.Partial(@"MenuBarItems/_UserProfile")
    </li>
                        
    <li>
        @Html.Partial(@"MenuBarItems/_Reports")
    </li>

    <li>
        @Html.Partial(@"MenuBarItems/_Tests")
    </li>
                    }

                    <li>
                        @Html.Partial(@"MenuBarItems/_LoginLogoff")
                    </li> 


                    
                    <li>
                    </li> 
                    <li class="dropdown">
                        
                    </li>



                </ul>
            </div><!--/.nav-collapse -->
        </div>
    </nav>

    <div class=" container theme-showcase" role="main">
        <div class="page-header"></div>
            @RenderBody()
   </div> <!-- /container -->

    
    @RenderSection("scripts", required: false)
</body>
</html>